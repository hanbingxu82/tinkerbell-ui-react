{
  "name": "tinkerbell-ui-react",
  "version": "0.3.1",
  "description": "一款简单而又优雅的开源 react 组件库",
  "author": "hanbingxu",
  "license": "MIT",
  "main": "dist/index.js",
  "private": false,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --css-modules false --no-compress --format modern,cjs && cp -r src/packages/Icon/fontcss/* ./dist ",
    "start": "microbundle-crl --css-modules false watch --no-compress --format modern,cjs & cp -r src/packages/Icon/fontcss/* ./dist",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "go": "npm publish --access=public"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@emotion/core": "^10.0.14",
    "@swiftcarrot/color-fns": "^3.0.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.179",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@xkit/popover": "^0.1.20",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "rc-tooltip": "^5.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-input-number": "^5.0.18",
    "react-input-slider": "^5.1.7",
    "react-scripts": "^3.4.1",
    "react-slick": "^0.28.1",
    "react-test-renderer": "^16.8.6",
    "tinycolor2": "^1.4.2",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "build": "^0.1.4",
    "classnames": "^2.3.1",
    "node-sass": "^4.14.1",
    "rc-animate": "^3.1.1",
    "rc-notification": "^4.5.7",
    "rc-tooltip": "^5.1.1",
    "rc-util": "^5.18.1",
    "react-input-number": "^5.0.18",
    "react-router-dom": "^6.2.2",
    "react-slick": "^0.28.1",
    "react-test-renderer": "^16.8.6",
    "run": "^1.4.0"
  }
}
